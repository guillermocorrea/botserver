{
  "name": "botserver",
  "version": "0.0.1",
  "description": "A event based server for guebot, based on websockets.",
  "main": "src/server.js",
  "directories": {
    "test": "test"
  },
  "engines": {
    "node": "0.10.x"
  },
  "scripts": {
    "start": "node src/server.js",
    "test": "grunt test",
    "ci": "istanbul cover -- grunt test && grunt coveralls",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "postinstall": "./node_modules/bower/bin/bower install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guillermocorrea/botserver.git"
  },
  "keywords": [
    "server",
    "websockets",
    "event"
  ],
  "author": "Luis Guillermo Correa",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/guillermocorrea/botserver/issues"
  },
  "homepage": "https://github.com/guillermocorrea/botserver",
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-contrib-jshint": "^0.10.0",
    "grunt-simple-mocha": "^0.4.0",
    "mocha": "^2.0.0",
    "should": "^4.1.0",
    "socket.io-client": "~1.1.0",
    "istanbul": "~0.3.2",
    "grunt-mocha-istanbul": "~2.2.0",
    "coveralls": "~2.11.2",
    "mocha-lcov-reporter": "0.0.1",
    "grunt-coveralls": "~1.0.0"
  },
  "dependencies": {
    "bcrypt-nodejs": "~0.0.3",
    "body-parser": "~1.9.2",
    "connect-flash": "~0.1.1",
    "cookie-parser": "~1.3.3",
    "express": "~4.9.8",
    "express-session": "~1.9.1",
    "coveralls": "~2.11.2",
    "grunt": "~0.4.5",
    "grunt-contrib-jshint": "~0.10.0",
    "grunt-coveralls": "~1.0.0",
    "grunt-mocha-istanbul": "~2.2.0",
    "grunt-simple-mocha": "~0.4.0",
    "mongoose": "~3.8.19",
    "morgan": "~1.4.1",
    "mysql": "~2.5.3",
    "passport": "~0.2.1",
    "passport-google-oauth": "~0.1.5",
    "passport-local": "~1.0.0",
    "istanbul": "~0.3.2",
    "socket.io": "~0.9.16",
    "mocha-lcov-reporter": "~0.0.1",
    "winston": "~0.8.3",
    "should": "~4.3.0",
    "bower": "~1.3.12",
    "socket.io-client": "~1.1.0"
  }
}
